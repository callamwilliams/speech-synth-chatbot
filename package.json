{
  "name": "voice-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "start:8080": "next start -p 8080",
    "test": "jest --maxWorkers=2 --verbose",
    "test:unit": "jest",
    "test:integration": "cypress run --browser chrome",
    "tdd": "npm run test:unit -- --watch --coverage=false --verbose",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "cover": "npm run cover:unit && npm run cover:integration && npm run cover:report",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:integration": "nyc --silent --no-clean npm run test:integration",
    "cover:report": "nyc report --reporter=lcov --reporter=text",
    "cover:merge": "npm run cover:unit && npm run cover:integration && nyc merge .nyc_output coverage.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:unit"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint ./src --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/plugin-transform-spread": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime-corejs3": "^7.13.10",
    "@testing-library/dom": "^7.30.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "^1.12.0",
    "core-js": "^3.9.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "growl": "^1.10.5",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-css-modules-transform": "^4.2.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-next-dynamic": "^1.0.1",
    "jest-styled-components": "^7.0.3",
    "jest-watch-typeahead": "^0.6.1",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-mock-store": "^1.5.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.1",
    "date-fns": "^2.19.0",
    "i": "^0.3.6",
    "immer": "^8.0.2",
    "next": "^10.0.9",
    "next-redux-wrapper": "^6.0.2",
    "nookies": "^2.5.2",
    "npm": "^7.6.3",
    "phosphor-react": "^1.2.1",
    "prop-types": "15.7.2",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-speech-recognition": "^3.7.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "snakecase-keys": "^3.2.1",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "swr": "^0.5.3",
    "uuid": "^8.3.2"
  }
}
